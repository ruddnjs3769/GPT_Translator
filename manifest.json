{
  "manifest_version": 3,
  "name": "AI Translator",
  "version": "1.0",
  "description": "Translate web pages using AI models.",
  "permissions": ["activeTab", "storage", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
